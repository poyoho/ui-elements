					"include": "#vue-interpolations"
				},
				{
					"include": "#template-tag"
				}
			]
		},
		"tag-stuff": {
			"begin": "\\G",
			"end": "(?=/>)|(>)",
			"endCaptures": {
				"1": {
					"name": "punctuation.definition.tag.end.vuehtml"
				}
			},
